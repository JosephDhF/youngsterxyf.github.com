---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<rss
    xmlns:atom="http://www.w3.org/2005/Atom" 
    version="2.0"
    >
    <channel>
        <title><![CDATA[{{ site.title }}]]></title>
        <atom:link href="http://youngsterxyf.github.com/BlackWhite/feed.xml" rel="self" type="application/rss+xml" />
        <link>http://{{ site.domain }}</link>
        <language>zh-CN</language>
        {% for post in site.posts limit:10 %}
        <item>
            <title><![CDATA[{{ post.title }}]]></title>
            <description type="html"><![CDATA[{{ post.content | xml_escape }}]]></description>
            <published><![CDATA[{{post.date | date_to_xmlschema }}]]></published>
            <link><![CDATA[http://{{ site.domain }}{{ post.url }}]]></link>
        </item>
        {% endfor %}
    </channel>
</rss>
